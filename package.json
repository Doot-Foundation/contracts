{
  "name": "doot_mina_contracts",
  "version": "0.2.0",
  "description": "Doot Oracle - Cryptographically verified price feeds on Mina Protocol with zero-knowledge proofs",
  "author": "Doot Foundation",
  "license": "Apache-2.0",
  "keywords": [
    "mina-zkapp",
    "mina-zk-app",
    "mina-dapp",
    "zkapp",
    "oracle",
    "price-feeds",
    "zero-knowledge"
  ],
  "type": "module",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "buildw": "tsc --watch",
    "coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "format": "prettier --write --ignore-unknown **/*",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:doot": "node --experimental-vm-modules node_modules/jest/bin/jest.js src/contracts/Doot.test.ts",
    "test:registry": "node --experimental-vm-modules node_modules/jest/bin/jest.js src/contracts/Registry.test.ts",
    "test:aggregation": "node --experimental-vm-modules node_modules/jest/bin/jest.js src/contracts/Aggregation.test.ts",
    "testw": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "lint": "npx eslint src/* --fix",
    "cache:build": "node build/src/cache/cache.js",
    "deploy:doot": "node build/src/deploy/doot_main.js",
    "deploy:registry": "node build/src/deploy/registry_main.js",
    "deploy:aggregation": "node build/src/deploy/aggregation_main.js"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "eslint-plugin-o1js": "^0.4.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^5.1.0",
    "axios": "^1.7.8",
    "dotenv": "^16.4.5",
    "o1js": "^2.9.0",
    "o1js-pack": "^0.8.0"
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
